{
  "name": "Standard Notes Extension",
  "version": "1.1",
  "description": "Standard Notes unofficial extension",
  "permissions": [
    "activeTab",
    "tabs",
    "https://app.standardnotes.org/"
  ],
  "background": {
    "service_worker": "src/background.js"
  },
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 3
}
